#maxint=1000.

% fish hamming numbers
fish(1,1,1).

fish(X,2,Y) :- fish(_,_,X), Y = 2*X.
fish(X,3,Y) :- fish(_,_,X), Y = 3*X.
fish(X,5,Y) :- fish(_,_,X), Y = 5*X.


% sail hamming numbers
sail(1,1,1).

sail(X,2,Y) :- sail(_,1,X), Y = 2*X.
sail(X,2,Y) :- sail(_,2,X), Y = 2*X.
sail(X,3,Y) :- sail(_,2,X), Y = 3*X.
sail(X,3,Y) :- sail(_,3,X), Y = 3*X.
sail(X,5,Y) :- sail(_,3,X), Y = 5*X.
sail(X,5,Y) :- sail(_,5,X), Y = 5*X.